---
import ContentsList from '@/components/common/ContentsList.astro';
import Layout from '@/layouts/Layout.astro';

interface PageFrontmatter {
  url: string;
  title: string;
}

const pages = await Astro.glob<PageFrontmatter>('./*/index.astro');

const pagetitle = 'TOP';
---

<Layout title={pagetitle}>
  <ContentsList contents={pages} />
</Layout>
